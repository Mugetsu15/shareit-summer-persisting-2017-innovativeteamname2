/*C3*/SET SCHEMA PUBLIC
drop table Book if exists
drop table Copy if exists
drop table Disc if exists
drop table hibernate_sequences if exists
drop table TableMedium if exists
drop sequence hibernate_sequence if exists
create sequence hibernate_sequence start with 1 increment by 1
create table Book (id bigint not null, title varchar(255), author varchar(255), isbn varchar(255), primary key (id))
create table Copy (COPY_ID bigint not null, medium varbinary(255), owner varchar(255), primary key (COPY_ID))
create table Disc (id bigint not null, title varchar(255), barcode varchar(255), director varchar(255), fsk integer not null, primary key (id))
create table hibernate_sequences (sequence_name varchar(255) not null, next_val bigint, primary key (sequence_name))
create table TableMedium (id bigint not null, title varchar(255), primary key (id))
